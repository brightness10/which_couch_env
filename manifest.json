{
  "name": "Which Couch Env?",
  "version": "1.0",
  "description": "Visualize which env are you on",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/_utils/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": ["declarativeContent", "activeTab", "tabs"],
  "manifest_version": 2
}